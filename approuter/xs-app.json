{
  "welcomeFile": "app/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/lib/~[^/]*~/(.*)$",
      "service": "html5-apps-repo-rt",
      "target": "myui5lib/$1",
      "cacheControl": "public, max-age=31536000"
    },
    {
      "source": "^/lib/(.*)$",
      "service": "html5-apps-repo-rt",
      "target": "myui5lib/$1",
      "cacheControl": "must-revalidate"
    },
    {
      "source": "^/app/resources/(.*)$",
      "target": "$1",
      "destination": "ui5"
    },
    {
      "source": "^/app/~[^/]*~/(.*)$",
      "service": "html5-apps-repo-rt",
      "target": "myui5app/$1",
      "cacheControl": "public, max-age=31536000"
    },
    {
      "source": "^/app/(.*)$",
      "service": "html5-apps-repo-rt",
      "target": "myui5app/$1",
      "cacheControl": "must-revalidate"
    }
  ]
}
